config_version=5

[application]

config/name="tunnels"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://assets/icons/icon.svg"

[autoload]

Events="*res://global/autoloads/events.gd"
PathGenerator="*res://global/autoloads/path_generator.gd"
GridLevel="*res://global/autoloads/grid_level.tscn"
EconomyManager="*res://global/autoloads/economy_manager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_state_charts/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "yellow",
"res://global/": "red",
"res://scenes/": "orange",
"res://scenes/_debug/": "gray",
"res://scenes/game/": "orange"
}

